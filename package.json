{
  "name": "webstock",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0",
    "npm": "^6.14.4"
  },
  "scripts": {
    "create-env": "echo runtime: \"nodejs10 \nenv_variables:\nDB_HOST: ${_DB_HOST}\nDB_PATH: ${_DB_PATH}\nDB_USER: ${_DB_USER}\nDB_PASS: ${_DB_PASS}\nDB_DATABASE: ${_DB_DATABASE}\nDATABASE_URL: ${_DATABASE_URL}\nSECRET: ${_SECRET}\nSENDGRID: ${_SENDGRID}\nPAGARME_KEY: ${_PAGARME_KEY}\" > app.yaml",
    "dev": "nodemon src/app.js",
    "start": "node src/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "essential-validation": "^1.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0",
    "pagarme": "^4.8.0",
    "react-input-mask": "^2.0.4",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "sequelize-paginate": "^1.1.6",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.0.4",
    "cz-conventional-changelog": "3.1.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.3"
  }
}
